{
  "name": "little-lemon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ui:coverage": "vitest --coverage --ui",
    "coverage": "cross-env MASTER_COVERAGE=true VITEST_COVERAGE=true vitest run",
    "coverage:ui": "cross-env MASTER_COVERAGE=true VITEST_COVERAGE=true vitest --ui",
    "coverage:report": "cross-env MASTER_COVERAGE=true node scripts/generate-coverage-report.js"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.26",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "axe-core": "^4.10.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.0",
    "vitest-axe": "^0.1.0"
  }
}
